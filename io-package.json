{
    "common": {
        "name":                     "mihome-vacuum",
        "version": "0.1.0",
        "news": {
			"0.1.0": {
                "en": 				"Inital version",
                "de": 				"Erste Version",
                "ru": 				"Первая версия"
            }
        },
        "title":                    "mihome-vacuum Adapter",
        "desc":                     "This adapter allows control Xiaomi Vacuum cleaner",
        "authors": [
            "bluefox <dogafox@gmail.com>"
        ],
        "license":                  "MIT",
        "platform":                 "Javascript/Node.js",
        "mode":                     "daemon",
        "loglevel":                 "info",
        "icon":                     "mihome-vacuum.png",
        "webExtension":             "lib/mihome-vacuum.js",
        "readme":                   "https://github.com/ioBroker/ioBroker.mihome-vacuum/blob/master/README.md",
        "keywords":                 ["url", "html", "file", "mihome-vacuum"],
        "extIcon":                  "https://raw.githubusercontent.com/ioBroker/ioBroker.mihome-vacuum/master/admin/mihome-vacuum.png",
		"type":                     "hardware",
        "config":{
            "minWidth": 800,
            "width ": 1224
        }
    },
    "native": {
        "ip":       "",
        "port":     54321,
        "ownPort":  56363,
        "start":    "",
        "pause":    "",
        "home":     "",
        "find":     "",
        "pingInterval": 20000,
        "level1":   "",
        "level2":   "",
        "level3":   ""
    },
    "instanceObjects": [
        {
            "_id":  "info",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id":  "info.consumable",
            "type": "channel",
            "common": {
                "name": "Vaccuum consumable"
            },
            "native": {}
        },
        {
            "_id":  "control",
            "type": "channel",
            "common": {
                "name": "Information"
            },
            "native": {}
        },
        {
            "_id":  "info.connection",
            "type": "state",
            "common": {
                "role":  "indicator.connected",
                "name":  "If connected to CUL",
                "type":  "boolean",
                "read":  true,
                "write": false,
                "def":   false
            },
            "native": {}
        },
        {
            "_id":  "control.start",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Start vacuum",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "control.pause",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Pause vacuum",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "control.home",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Go home",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
            "_id":  "control.find",
            "type": "state",
            "common": {
                "role":  "button",
                "name":  "Find",
                "type":  "boolean",
                "read":  false,
                "write": true
            },
            "native": {}
        },
        {
          "_id":  "info.battery",
          "type": "state",
          "common": {
              "name":  "Battery status",
              "type":  "number",
              "read":  true,
              "write": false,
              "unit":  "%",
              "min":   0,
              "max":   100
          },
          "native": {}
        },
        {
          "_id":  "info.cleanedarea",
          "type": "state",
          "common": {
              "name":  "Cleaned area",
              "type":  "number",
              "read":  true,
              "write": false,
              "unit":  "m²",
              "min":   0
          },
          "native": {}
        },
        {
          "_id":  "info.cleanedtime",
          "type": "state",
          "common": {
              "name":  "Cleaning time",
              "type":  "number",
              "read":  true,
              "write": false,
              "unit":  "min",
              "min":   0
          },
          "native": {}
        },
        {
          "_id":  "control.fan_power",
          "type": "state",
          "common": {
              "name":  "Suction power",
              "type":  "number",
              "role":  "level",
              "read":  true,
              "write": true,
              "unit":  "%",
              "min":   0,
              "max":    100
          },
          "native": {}
        },
        {
        "_id":  "info.state",
        "type": "state",
        "common": {
            "name":  "Vacuum state",
            "type":  "number",
            "read":  true,
            "write": false,
            "min":   0,
            "max":   30,
            "states":  {
                       "1": "Unknown 1",
                       "2": "Sleep no Charge",
                       "3": "Sleep",
                       "5": "Cleaning",
                       "6": "Returning home",
                       "8": "Charging",
                       "10": "Paused",
                       "11": "Spot cleaning",
                       "12": "Error?!"
                     }
        },
        "native": {}
      },
      {
      "_id":  "info.error",
      "type": "state",
      "common": {
          "name":  "Error",
          "type":  "number",
          "read":  true,
          "write": false,
          "min":   0,
          "max":   30,
          "states":  {
                      "0": "No error",
                      "1": "Laser distance sensor error",
                      "2": "Collision sensor error",
                      "3": "Wheels on top of void, move robot",
                      "4": "Clean hovering sensors, move robot",
                      "5": "Clean main brush",
                      "6": "Clean side brush",
                      "7": "Main wheel stuck?",
                      "8": "Device stuck, clean area",
                      "9": "Dust collector missing",
                      "10": "Clean filter",
                      "11": "Stuck in magnetic barrier",
                      "12": "Low battery",
                      "13": "Charging fault",
                      "14": "Battery fault",
                      "15": "Wall sensors dirty, wipe them",
                      "16": "Place me on flat surface",
                      "17": "Side brushes problem, reboot me",
                      "18": "Suction fan problem",
                      "19": "Unpowered charging station"
                    }
      },
      "native": {}
    },
    {
      "_id":  "info.dnd",
      "type": "state",
      "common": {
          "name":  "DnD aktiv",
          "type":  "switch",
          "role":  "level",
          "read":  true,
          "write": false
      },
      "native": {}
    },
    {
      "_id":  "info.consumable.main_brush",
      "type": "state",
      "common": {
          "name":  "Main brush",
          "type":  "number",
          "role":  "level",
          "read":  true,
          "write": false,
          "unit":  "%",
          "min":  0
      },
      "native": {}
    },
    {
      "_id":  "info.consumable.side_brush",
      "type": "state",
      "common": {
          "name":  "Main brush",
          "type":  "number",
          "role":  "level",
          "read":  true,
          "write": false,
          "unit":  "%",
          "min":  0
      },
      "native": {}
    },
    {
      "_id":  "info.consumable.filter",
      "type": "state",
      "common": {
          "name":  "Filter",
          "type":  "number",
          "role":  "level",
          "read":  true,
          "write": false,
          "unit":  "%",
          "min":  0
      },
      "native": {}
    }

    ]

}
